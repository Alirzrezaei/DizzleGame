package logic;

/**
 *
 * @author Alireza
 */
public class Winner {
    int winnerID;
    int winnerPoint;
    
    
    Winner(int winnerID, int winnerPoint){
        this.winnerID = winnerID;
        this.winnerPoint = winnerPoint;
    }
    protected void setWinnerID(int winnerID){
        this.winnerID = winnerID;
    }
    protected void setWinnerPoint(int winnerPoint){
        this.winnerPoint = winnerPoint;
    }
    public int getWinnerID(){
        return this.winnerID;
    }
    public int getWinnerPoint(){
        return this.winnerPoint;
    }
}
